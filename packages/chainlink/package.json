{
  "name": "@chainlink-io/chainlink",
  "version": "0.0.4",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "pkg-utils build",
    "watch": "pkg-utils watch"
  },
  "bin": {
    "chainlink": "./dist/cli/index.js"
  },
  "files": [
    "./dist*",
    "./dist/app/*",
    "./dist/cli/*",
    "./dist/app/packages/@chainlink/app/.next",
    "./dist/app/packages/@chainlink/app/*"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@inquirer/prompts": "^5.3.8",
    "argparse": "^2.0.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.10.10",
    "@types/node": "22.x.x"
  },
  "peerDependencies": {
    "next": "^14.2.4",
    "typescript": "^5.0.0"
  }
}
